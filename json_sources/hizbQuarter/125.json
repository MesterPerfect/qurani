			"ruku": 270,
			"hizbQuarter": 125,
			"sajda": false
		},
		{
			"number": 2368,
			"text": "فَأَلْقَاهَا فَإِذَا هِيَ حَيَّةٌ تَسْعَىٰ",
			"surah": {
				"number": 20,
				"name": "سُورَةُ طه",
				"englishName": "Taa-Haa",
				"englishNameTranslation": "Taa-Haa",
