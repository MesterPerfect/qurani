			"sajda": false
		},
		{
			"number": 2357,
			"text": "وَهَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
			"surah": {
				"number": 20,
				"name": "سُورَةُ طه",
				"englishName": "Taa-Haa",
				"englishNameTranslation": "Taa-Haa",
