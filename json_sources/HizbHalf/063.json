			"ruku": 270,
			"hizbQuarter": 125,
			"sajda": false
		},
		{
			"number": 2359,
			"text": "فَلَمَّا أَتَاهَا نُودِيَ يَا مُوسَىٰ",
			"surah": {
				"number": 20,
				"name": "سُورَةُ طه",
